CREATE TABLE `aux_fjbzfp` (
  `fjbzfpId` bigint(10) NOT NULL AUTO_INCREMENT COMMENT '辅警编制分配情况ID',
  `orgId` bigint(20) DEFAULT NULL COMMENT '单位ID',
  `orgName` varchar(64) DEFAULT NULL COMMENT '工作单位名称',
  `fjType` varchar(100) DEFAULT NULL COMMENT '辅警类别,字典AUX_FJLB（1：专项辅警；2：联防辅警；4：普通辅警）',
  `state` varchar(64) DEFAULT NULL COMMENT '状态,字典FJ_DASTATE（1：在岗；2：离职）',
  `existingPeople` varchar(64) DEFAULT NULL COMMENT '现有人数',
  `formationPeople` varchar(64) DEFAULT NULL COMMENT '编制人数',
  PRIMARY KEY (`fjbzfpId`)
) ENGINE=InnoDB AUTO_INCREMENT=122413001998337 DEFAULT CHARSET=utf8mb4