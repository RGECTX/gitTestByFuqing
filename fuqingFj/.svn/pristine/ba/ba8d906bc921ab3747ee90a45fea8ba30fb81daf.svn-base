	<script type="text/javascript">
		$(document).ready(function() {
			moveJrxzfjlb();
			function moveJrxzfjlb(){
				var stop=false;
				setInterval(function(){
				   if(stop==true || document.getElementById('jrxzfjlb').getElementsByTagName('li').length<4){
						return;
					}
				   doscroll()
				}, 3000);
			   function doscroll(){
					var $parent = $('#jrxzfjlb');
					var $first = $parent.find('li:first');
					var height = $first.height();
					$first.animate({
						height: 0
						}, 500, function() {
						$first.css('height', height).appendTo($parent);
					});    
			   };
				//鼠标放到快递信息(ul)上时
				$('#jrxzfjlb').hover(function(){
					$(this).css("cursor","pointer");
					stop=true;//停止动画
				},function(){
					stop=false;//开始动画
				});
			}
			moveJyjl();
			function moveJyjl(){
				var stop=false;
				setInterval(function(){
				   if(stop==true || document.getElementById('jyjl').getElementsByTagName('li').length<6){
						return;
					}
				   doscroll()
				}, 3000);
			   function doscroll(){
					var $parent = $('#jyjl');
					var $first = $parent.find('li:first');
					var height = $first.height();
					$first.animate({
						height: 0
						}, 500, function() {
						$first.css('height', height).appendTo($parent);
					});    
			   };
				//鼠标放到快递信息(ul)上时
				$('#jyjl').hover(function(){
					$(this).css("cursor","pointer");
					stop=true;//停止动画
				},function(){
					stop=false;//开始动画
				});
			}
		})
	</script>
